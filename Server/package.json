    "@nestjs/config": "^3.2.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/terminus": "^10.2.3",
    "@prisma/client": "5.18.0",
    "@sentry/node": "^7.112.2",
    "@sentry/profiling-node": "^7.112.2",
    "axios": "^1.7.4",
